# Test configuration for pump automation (MOCK_MODE)
MOCK_MODE=true
LOG_LEVEL=INFO

# LED Configuration
LED_COUNT=30
LED_PIN=18

# MQTT (disabled for local testing)
MQTT_ENABLED=false

# Temperature sensors
TEMP_ENABLED=true
TEMP_UPDATE_INTERVAL=60

# Relay configuration
RELAY_ENABLED=true
RELAY_CONFIG=pump:Aquarium Pump:17:true:OFF:60
RELAY_WATCHDOG_ENABLED=true
RELAY_WATCHDOG_INTERVAL=30

# Water level sensor
WATER_LEVEL_ENABLED=true
WATER_LEVEL_PIN=23
WATER_LEVEL_ACTIVE_HIGH=true
WATER_LEVEL_DEBOUNCE_TIME=0.5

# Pump automation
PUMP_AUTOMATION_ENABLED=true
PUMP_RELAY_ID=pump
PUMP_ON_INTERVAL=10
PUMP_OFF_INTERVAL=10
PUMP_MAX_RUNTIME=60
